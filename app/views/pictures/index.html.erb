<h1>Hello</h1>

<!-- "-" says run something in ruby, but don't print the result -->
<%- @pictures.each do |picture| %>
		<h3><%= picture.title %></h3>
		<h4><%= picture.artist %></h5>
		<p>
			<img src="<%= picture.url %>"> <br>

			<a href="/pictures/<%= picture.id %>">View Photo</a> | <a href="/pictures/<%= picture.id %>/edit">Edit</a>
		</p>
<% end %>	
